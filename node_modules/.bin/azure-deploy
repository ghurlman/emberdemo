#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../ember-cli-azure-deploy/bin/azure-deploy" "$@"
  ret=$?
else 
  node  "$basedir/../ember-cli-azure-deploy/bin/azure-deploy" "$@"
  ret=$?
fi
exit $ret
